{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User",
    "name": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "organization": {
      "type": "string"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "picture": {
      "type": "media",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "multiple": false,
      "required": false
    },
    "userrole": {
      "type": "string"
    },
    "programarea": {
      "type": "string"
    },
    "roleInProgramArea": {
      "type": "string"
    },
    "workgroup": {
      "type": "enumeration",
      "enum": [
        "RENCI",
        "RTI_INTERNATIONAL",
        "COLLECTIVE_BOARD"
      ]
    },
    "bio": {
      "type": "text"
    },
    "linkedin": {
      "type": "string"
    },
    "RoleInHeal": {
      "type": "enumeration",
      "enum": [
        "Representative_of_a_Community_Organization",
        "Community_Member",
        "Working_on_a_HEAL_funded_program",
        "Non_HEAL_Researcher",
        "Collective_Board_Member",
        "Co_PI_HEAL_Data_Stewardship_Group",
        "External_Ethics_Expert",
        "Clinician",
        "Agency",
        "Partner",
        "Other"
      ]
    },
    "current": {
      "type": "boolean"
    },
    "previouslyConfirmed": {
      "type": "boolean"
    }
  }
}
